; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "AbStatic",
"class": "AbNim",
"language": "GDScript",
"path": "res://addons/aban/statics/nim.gd"
}, {
"base": "Object",
"class": "AbStatic",
"language": "GDScript",
"path": "res://addons/aban/statics/static.gd"
} ]
_global_script_class_icons={
"AbNim": "",
"AbStatic": ""
}

[application]

config/name="Aban"
run/main_scene="res://g1939_nimatron/main.tscn"

[autoload]

AbColor="*res://addons/aban/singletons/ab_color.gd"

[debug]

settings/stdout/verbose_stdout=true

[display]

window/size/width=800
window/handheld/orientation="sensor_portrait"
window/stretch/mode="2d"
window/stretch/aspect="keep_width"

[editor_plugins]

enabled=PoolStringArray( "aban" )

[input_devices]

pointing/emulate_touch_from_mouse=true

[rendering]

quality/driver/driver_name="GLES2"
threads/thread_model=2
vram_compression/import_etc=true
vram_compression/import_etc2=false
environment/default_clear_color=Color( 0, 0, 0, 1 )
